    End Function

    Public Function NewMsgBox(ByVal sMessage As String, ByVal sCaption As String, Optional ByVal mFont As Font = Nothing, Optional ByVal BGColor As Brush = Nothing, Optional ByVal buttonz As MsgBoxBtns = Nothing, Optional ByVal sIcon As Bitmap = Nothing) As String
        frmMsg = New Form()
        If mFont Is Nothing Then msgFont = New Font("Arial", 12, FontStyle.Regular, GraphicsUnit.Pixel)
        If BGColor Is Nothing Then BGColor = Brushes.DimGray
        Dim stringsize As SizeF
        Dim sideBuffer As Short = 10
        Dim icoW As Short = 0, icoH As Short = 0
        If Not bmpIcon Is Nothing Then
            icoW = bmpIcon.Width
            icoH = bmpIcon.Height
        End If
        frmMsg.MaximizeBox = False
        frmMsg.MinimizeBox = False
        frmMsg.Text = sCaption
        Dim bmpBackground As New Bitmap(1, 1)
        frmLeft = Form.ActiveForm.Left + (Form.ActiveForm.Width / 2)
        frmTop = Form.ActiveForm.Top + (Form.ActiveForm.Height / 2)

        Using gx As Graphics = Graphics.FromImage(bmpBackground)
            stringsize = gx.MeasureString(sMessage, msgFont, New SizeF(frmLayoutWidth - icoW, 600))
        End Using
        frmW = CInt(stringsize.Width) + sideBuffer * 2 + 4
        frmH = CInt(stringsize.Height) + 30 + (sideBuffer * 2) + icoH
        If frmW < 132 Then frmW = 132
        bmpBackground = New Bitmap(frmW + sideBuffer * 2 + icoW, frmH)
        Dim iLolz As Short = 0
        If icoW <> 0 Then iLolz = sideBuffer
        Dim drawRect As New Rectangle(sideBuffer + (((frmW - sideBuffer * 2) - stringsize.Width) / 2) + icoW, sideBuffer, frmW - iLolz, stringsize.Height)
        Using gx As Graphics = Graphics.FromImage(bmpBackground) '(frmW / 2) - (stringsize.Width / 2) - 4, 12
            gx.FillRectangle(BGColor, New Rectangle(0, 0, bmpBackground.Width, bmpBackground.Height))
            If Not bmpIcon Is Nothing Then gx.DrawImage(bmpIcon, sideBuffer, sideBuffer, bmpIcon.Width, bmpIcon.Height)
            gx.DrawString(sMessage, msgFont, msgBrush, drawRect)
            frmMsg.BackgroundImage = bmpBackground
        End Using
        frmLeft -= (frmMsg.Width / 2)
        frmTop -= (frmMsg.Height / 2)
        frmW += sideBuffer + icoW

        Dim btnTop As Short = Math.Max(stringsize.Height, icoH) + (sideBuffer * 2)

        If buttonz.btnz.Count = 1 Then
            'buttonz.btnz(0).Left = (frmW / 2) - (buttonz.btnz(0).Width / 2)
        End If

        Dim iLeft As Short = 0

        For i = 0 To buttonz.btnz.Count - 1
            iLeft += buttonz.btnz(i).Width + 16
            buttonz.btnz(i).Parent = frmMsg
            buttonz.btnz(i).Left = frmW - 16 - iLeft
            buttonz.btnz(i).Top = btnTop
            AddHandler buttonz.btnz(i).Click, AddressOf btn_Click
            frmMsg.Controls.Add(buttonz.btnz(i))
        Next
        If buttonz.btnz.Count > 0 Then frmH = buttonz.btnz(0).Bottom + sideBuffer + 35

        AddHandler frmMsg.Load, AddressOf frmMsg_Load
        frmMsg.ShowDialog()

        Return sResult
    End Function

    Private Sub frmMsg_Load(ByVal sender As System.Object, ByVal e As System.EventArgs)
        frmMsg.Left = frmLeft
        frmMsg.Top = frmTop
        frmMsg.Width = frmW
        frmMsg.Height = frmH
        frmMsg.FormBorderStyle = FormBorderStyle.Fixed3D
    End Sub

    Private Sub btn_Click(ByVal sender As Object, ByVal e As System.EventArgs)
        Dim btnTemp As Button = sender
        sResult = btnTemp.Tag
        frmMsg.Close()
    End Sub

